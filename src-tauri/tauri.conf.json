{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/logo.png", "icons/logo.ico"]
  },
  "productName": "Version Manager",
  "mainBinaryName": "Version Manager",
  "version": "0.0.3",
  "identifier": "version-manager",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Version Manager",
        "width": 800,
        "height": 600,
        "minWidth": 750,
        "minHeight": 450,
        "center": true,
        "resizable": true,
        "decorations": false,
        "titleBarStyle": "Visible",
        "transparent": true
      }
    ]
  }
}
