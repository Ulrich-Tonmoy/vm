{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "version-manager",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": ["**"]
      }
    },
    "windows": [
      {
        "title": "Version Manager",
        "width": 800,
        "height": 600,
        "center": true,
        "minWidth": 500,
        "minHeight": 350,
        "resizable": true,
        "decorations": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "Version Manager",
      "icon": ["icons/logo.png", "icons/logo.ico"]
    }
  }
}
